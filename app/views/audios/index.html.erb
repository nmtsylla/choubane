<h1>Listing audios</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Author</th>
      <th>Date</th>
      <th>Image</th>
      <th>Piste</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <%= render partial: 'player' %>
  <tbody>
    <% @audios.each do |audio| %>
      <tr>
        <td><%= audio.user %></td>
        <td><%= audio.author %></td>
        <td><%= audio.date %></td>
        <td><%= audio.image %></td>
        <td><%= audio.piste %></td>
        <td><%= link_to 'Show', audio %></td>
        <td><%= link_to 'Edit', edit_audio_path(audio) %></td>
        <td><%= link_to 'Destroy', audio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

