<h1>Listing categorie_audios</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categorie_audios.each do |categorie_audio| %>
      <tr>
        <td><%= categorie_audio.name %></td>
        <td><%= categorie_audio.description %></td>
        <td><%= categorie_audio.user %></td>
        <td><%= link_to 'Show', categorie_audio %></td>
        <td><%= link_to 'Edit', edit_categorie_audio_path(categorie_audio) %></td>
        <td><%= link_to 'Destroy', categorie_audio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Categorie audio', new_categorie_audio_path %>
